keywords = {Mobility,Multi-agent simulation,Simulation,Testbed,Transport},
mendeley-groups = {Microsimulation},
pages = {808--815},
publisher = {Elsevier B.V.},
title = {{Agent-based simulation testbed for on-demand mobility services}},
volume = {32},
year = {2014}
}

